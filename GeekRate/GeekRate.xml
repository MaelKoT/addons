						<Size><AbsDimension x="20" y="18" /></Size><Anchors><Anchor point="RIGHT" /></Anchors></HighlightTexture>
							</Button>
					</Frames>
						<Scripts>
						  <OnLoad>
							this:SetScale(0.85);
							local my_frame = this
							my_frame.text:SetText(UnitFactionGroup("player"))
							local cb_init_fn = function(self, level)
								if GeekRate_Faction_Get then
									local facts={"Alliance", "Horde", "Both"}
									local now_fact=GeekRate_Faction_Get()
									for i=1,#facts do
										local name = facts[i] 
										local info = UIDropDownMenu_CreateInfo();
										info.checked = false;
										if now_fact == name then 
											info.checked = true 
										end;
										info.text = name;
										info.func = function()
											my_frame.text:SetText(name)
											GeekRate_Faction_Set(name)
										end;
										UIDropDownMenu_AddButton(info, level);
									end
								end
							end
							UIDropDownMenu_Initialize(this, cb_init_fn);
					</OnLoad>
						  <OnHide>CloseDropDownMenus();</OnHide>
						</Scripts>
